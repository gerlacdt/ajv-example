{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://foobar/person.schema.json",
  "type": "object",
  "properties": {
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "address": {
      "type": "object",
      "properties": {
        "street": {
          "type": "string"
        },
        "city": {
          "type": "string"
        }
      },
      "required": ["city"]
    }
  },
  "required": ["lastName"]
}
